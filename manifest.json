{
    "manifest_version": 3,
    "name": "SimpleClock",
    "version": "1.0",

    "description": "A clock for the chrome extension menu",
    "author": "Theo Vanderkooy",

    "permissions": ["alarms", "storage"],
    "background": {
        "service_worker": "clock.js",
        "type": "module"
    },
    "options_page": "settings.html",
    "action": {
        "default_popup": "settings.html"
    }
}
